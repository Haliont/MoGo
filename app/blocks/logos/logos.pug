mixin logos()
  div.logos&attributes(attributes)
    .logos__inner
      .logos__img-wrap
        img.logos__img(src="img/logos/1.png", alt="Логотипчик")
        img.logos__img(src="img/logos/2.png", alt="Логотипчик")
      .logos__img-wrap
        img.logos__img(src="img/logos/3.png", alt="Логотипчик")
        img.logos__img(src="img/logos/4.png", alt="Логотипчик")
      .logos__img-wrap
        img.logos__img(src="img/logos/5.png", alt="Логотипчик")
        img.logos__img(src="img/logos/6.png", alt="Логотипчик")
